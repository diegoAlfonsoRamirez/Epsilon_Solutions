<%- include('templates/sidebarMain.ejs') %>
<% if (rol == 2) { %>
    <%- include('templates/sidebarPanelLead.ejs') %>
<% } %>
<%- include('templates/sidebarEnd.ejs') %>

<link rel="stylesheet" href="/css/sidebar.css" />
<link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
<link rel="stylesheet" href="/css/buttons.css" />
<link rel="stylesheet" href="/css/section.css" />

<div>
    <section class="section-card">
        <% for (let nombreM of nombreMs) { %>
        <h1 id="principal">Evaluaci√≥n de: <%- nombreM.nombre %> <%- nombreM.apellidoP %> </h1>
        <% } %>
        <hr class="db-hr">

        <% for (let nombreE of nombreEs) { %>
        <h3>Evaluador: <%- nombreE.nombre %> <%- nombreE.apellidoP %></h3>
        <% } %>
        <br>
        <table class="table table-striped" class="centered">
            <tr>
                <th>Prgunta:</th>
                <th>Respuesta:</th>
            </tr>
            <tbody>
                <% for (let answer of answers) { %>
                <tr>
                    <td><%- answer.Pregunta -%></td>
                    <td><%- answer.Respuesta -%></td>
                </tr>
                <% } %>
            </tbody>
        </table>

        <br>
        <a href="/mentees/misMentorados"><button type="button" class="btn btn-secondary">Regresar a tus Mentorados</button></a>
    </section>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
<script src="/bootstrap/js/bootstrap.min.js"></script>
<%- include('templates/footer.ejs') %>