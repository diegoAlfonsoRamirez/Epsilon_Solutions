<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
<link rel="stylesheet" href="/css/sidebar.css" />
<link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
<link rel="stylesheet" href="/css/buttons.css" />
<link rel="stylesheet" href="/css/section.css" />

<%- include('templates/sidebar.ejs') %>

<body style="background: url('/img/bg-sire.png');">
    <div class="main">
        <% for (let cuestionario of cuestionarios) { %>
        <section class="section-card">
                <h1 id="principal">Estás Evaluando a: <%-cuestionario.idEvaluado-%></h1>
                <hr class="db-hr">
                <br><br>
                <!-- Para contestar las preguntas -->
                <p>Contesta las siguientes preguntas de la manera más honesta: </p>
                <br><br>
                <% for (let feedback of feedbacks) { %>
                    <label for="Respuesta"><%- feedback.Pregunta -%></label>
                    <input type="text" id="Respuesta" name="Respuesta" > 
                    <br><br>
                <%} %>       
            </form>
        </section>
        <%} %> 
    </div>
</body>

<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
<script src="/bootstrap/js/bootstrap.min.js"></script>